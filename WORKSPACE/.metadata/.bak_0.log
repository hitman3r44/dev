	public Double TryParseDouble(String str){
		str=getStrCleaned(str);
		Double result=null;
	    try {
	    	result = Double.parseDouble(str);
	        return result;
	    } catch(NumberFormatException e) {
	        return -1d;
	    }
	}

	
	
	/**========================================================================
