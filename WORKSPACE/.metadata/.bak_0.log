	public UserSessionDTO getUserSessionById(List<UserSessionDTO> userList,Long userId){
		UserSessionDTO result=null;
		for(UserSessionDTO us:userList){
			if(us.getId()!=0 && us.getId()==userId){
				result=us;
				break;
			}
		}
		return result;
	}

	
	
	/**========================================================================
