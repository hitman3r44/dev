	String getStrCleaned(String str){
		if(str==null)str="";
		str = str.replaceAll("\\s","");
		str=str.replaceAll("\'","");
		str=str.replaceAll("\"","");
		str=str.replaceAll(";","");
		str=str.replaceAll(",","");
		return str;
	}
	
	
	/**========================================================================
