			case 2:
				orderDTO.setClientFirstName("");
				orderDTO.setClientLastName(elems[1]);
				break;
			case 3:
				orderDTO.setClientFirstName(elems[2]);
				orderDTO.setClientLastName(elems[1]);
				break;
			case 4:
				orderDTO.setClientFirstName(elems[3]);
