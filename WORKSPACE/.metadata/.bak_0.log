	public String removeUnusedSpaces(String srtValue) {
		String result = srtValue.replaceAll("^\\s+", "").replaceAll("^\\t+", "")	//Remove Left  Spaces and Tabs
								.replaceAll("\\s+$", "").replaceAll("\\t+$", "")	//Remove Right Spaces and Tabs
