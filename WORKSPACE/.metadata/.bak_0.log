					CLIENT_STREET_INF="?????";
				}
				data+="\""+CLIENT_STREET_INF+"\",";


				// 10: CLIENT_STREET1 (must be lest than 44 letters)
				if(orderDTO.getClientStreet()!=null)CLIENT_STREET1=orderDTO.getClientStreet();
