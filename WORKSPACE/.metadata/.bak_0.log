				if(CLIENT_LAST_NAME.length()>40){
					CLIENT_LAST_NAME="?????";
				}
				data+="\""+CLIENT_LAST_NAME+"\",";


				// 7: TITLE_NAME (must be lest than 44 letters)
				data+="\""+DEPT+"\",";


