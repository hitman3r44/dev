body, ul, li, input, select, textarea {
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:11px;
	color:#555555;
}
body, h1 {
	margin:0px;
	padding:0px;
	font-weight:normal;
	}
a {
	color:#2c8cab;
}
a:hover {
	color:#999999;
}
img {
	border:0;
}
table {
	margin-bottom : 0;
}
.right {
	text-align: right;
}
.hcenter {
	text-align:center;
}
.blue {
	color:#2c8cab;
	}
.black {
	color:#000000;
}
.bold {
	font-weight:bold;
}
.f10 {
	font-size:10px;
}
.f11 {
	font-size:11px;
}
.f12 {
	font-size:12px;
}
.f14 {
	font-size:14px;
}
.f16 {
	font-size:16px;
}
.f18 {
	font-size:18px;
}
.nav ul {
	margin:0;
}
.nav li {
	background:none repeat scroll 0 0 #777777;
	display:block;
	float:left;
	margin:1px 1px 0px 0px;
	height:26px;
	color:#FFFFFF;
}
.nav a {
	color:#FFFFFF;
	display:block;
	font-weight:bold;
	padding:6px 12px;
	text-decoration:none;
}
.nav a:visited {
	color:#FFFFFF;
	display:block;
	font-weight:bold;
	padding:6px 12px;
	text-decoration:none;
}
.nav li:hover {
	background-color:#39a6c8;
	color:#FFFFFF;
}
table.pad_2px td, table.pad_2px th  {
	padding:2px 10px 2px 0px;
	text-align:left;
	vertical-align:top;
}
div.form_height div {
	height:27px;
	text-align:left;
}
div.form_padding div {
	padding-top:4px;
}
tr.white_bg td, tr.white_bg th {
	background-color:#ffffff;
}
tr.blue_bg td, tr.blue_bg th {
	background-color:#f5f9fc;
	border-width:1px 0px 1px 0px;
	border-style:solid;
	border-color:#dfebf5;
}
tr.dark_blue_bg td, tr.dark_blue_bg th {
	background-color:#dee9fd;
}
tr.gray_bg td, tr.gray_bg th {
	background-color:#f7f7f7;
	border-width:1px 0px 1px 0px;
	border-style:solid;
	border-color:#ececec;
}
table.nowraptable td, table.nowraptable th, {
	white-space:nowrap;
}

table.pad_5px td, table.pad_5px th  {
	padding:5px 20px 5px 10px;
	vertical-align:top;
}
div.pad_2px div {
	padding:2px 30px 2px 10px;
	vertical-align:top;
}
.section_underline {
	border-width:0px 0px 1px 0px;
	border-style:solid;
	border-color:#ececec;
}
.section_aboveline {
	border-width:1px 0px 0px 0px;
	border-style:solid;
	border-color:#ececec;
}
.section_above_under {
	border-width:1px 0px 1px 0px;
	border-style:solid;
	border-color:#ececec;
}
.pa5 {
	padding-right:5px;
}
.pa10 {
	padding-right:10px;
}
.pa20 {
	padding-right:20px;
}
.pb5 {
	padding-left:5px;
}
.pb10 {
	padding-left:10px;
}
.pb20 {
	padding-left:20px;
}
.pb60 {
	padding-left:60px;
}
div.pbd10 div {
	padding-left:10px;
}
div.pbd20 div {
	padding-left:20px;
}
.gray_bg {
	background-color:#f7f7f7;
}
.gray_border {
	border-width:1px;
	border-style:solid;
	border-color:#ececec;
}
.blue_bg {
	background-color:#f5f9fc;
}
.blue_border {
	border-width:1px;
	border-style:solid;
	border-color:#dfebf5;
}
.green_border {
	border-width:1px;
	border-style:solid;
	border-color:#00FF00;
}
.white_bg {
	background-color:#ffffff;
}
.blue_header {
	background-image:url(../images/header_blue_bg.gif);
	color:#ffffff;
	font-weight:bold;
}
.gray_header {
	background-image:url(../images/header_gray_bg.gif);
	color:#ffffff;
	font-weight:bold;
}
.section_title {
	font-size:26px;
	color:#2c8cab;
	padding:20px 0px 3px 60px;
	margin-bottom:40px;
	border-bottom-width: 1px;
	border-bottom-style: dotted;
	border-bottom-color: #c7c7c7;
}
.pop_title {
	font-size:20px;
	color:#2c8cab;
	padding:0px 0px 3px 20px;
	border-bottom-width: 1px;
	border-bottom-style: dotted;
	border-bottom-color: #c7c7c7;
}
img.enlarge {
	border-width:0;
	display:inline;
	margin:0 0 2px -19px;
}
img.linkable {
	cursor: pointer;
}
.manuel_setting_price {
	color:#980000 ;
}