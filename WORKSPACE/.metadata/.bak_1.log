				CATEGORY_NAME="";
				BRAND_NAME="";
				MSRP="";
				OUT_COST="";
				PRICE="";
				PROFIT=0D;
				DISCOUNT=0D;
				QT_3MONTHS=0D;
				CLASSIFICATION=0D;
