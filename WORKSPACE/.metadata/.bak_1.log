				labelDISCOUNT= new Label(COL_DISCOUNT-1, row, DISCOUNT.toString(),cellFormat);
				cellDISCOUNT = (WritableCell) labelDISCOUNT;
				sheetToEdit.addCell(cellDISCOUNT);
				
				//10:COL_QT_3MONTHS
