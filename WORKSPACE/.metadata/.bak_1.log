	List<Long> getTabUsersIdFromStrUserId(String strUsersId,List<UserSessionDTO> userNameList,boolean saleForAllCleint){
		List<Long> resultList=new ArrayList<Long>();
		if(strUsersId==null)strUsersId="";
		if(saleForAllCleint){
			for(UserSessionDTO usrDTO:userNameList){
				if(usrDTO!=null)resultList.add(usrDTO.getId());
