			if(!strTabUsersId[i].equals("")){
				Long item=Long.parseLong(strTabUsersId[i]);
				resultList.add(item);
			}
		}
		return resultList;
	}
	
	
	/**========================================================================
 							getStrCleaned
	   ========================================================================*/
	String getStrCleaned(String str){
		if(str==null)str="";
		str = str.replaceAll("\\s","");
		str=str.replaceAll("\'","");
		str=str.replaceAll("\"","");
		str=str.replaceAll(";","");
		str=str.replaceAll(",","");
		return str;
	}
	
	
	/**========================================================================
