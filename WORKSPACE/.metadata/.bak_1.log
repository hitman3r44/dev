	public Date getDateFromStr(String strDate,SimpleDateFormat frm){
		Date result=null;
		strDate=getStrCleaned(strDate);
	    try {
	    	result=frm.parse(strDate);
	        return result;
	    } catch(Exception e) {
	        return null;
	    }

	}

